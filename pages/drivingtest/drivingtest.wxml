<!--pages/drivingtest/drivingtest.wxml-->
<view wx:for="{{useData}}" wx-for-index="index" wx-for-item="item">
  <text selectable="true">{{item.question}}</text>
  <image style="width: 200px; height: 200px; background-color: #eeeeee;" src="{{item.url}}"></image>
  <radio-group>

    <radio>{{item.item1}}</radio>
    <radio>{{item.item2}}</radio>
    <radio>{{item.item3}}</radio>
    <radio>{{item.item4}}</radio>
  </radio-group>
  

</view>
